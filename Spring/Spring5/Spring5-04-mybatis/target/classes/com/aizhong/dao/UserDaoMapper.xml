        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aizhong.dao.UserDao">


    <select id="save" parameterType="user">
        insert into mybatis.user(name, pwd) values (#{name}, #{pwd})
    </select>
</mapper>